											{if isNonnull($item.href)}
												{$item.href}
											{else}
												http://intranet.ec.gc.ca/default.asp?lang=En
											{/if}
											">
											{if isNonnull($item.acronym)}
												<abbr title="{$item.acronym}">
											{/if}
											<span>
											<span class="bold-gc">
											{if isNonnull($item.boldText)}
												{$item.boldText}
											{else}
												GC
											{/if}
											</span> 
											{if isNonnull($item.acronym)}
												</abbr>
											{/if}
											{if isNonnull($item.text)}
												{$item.text} 
											{else}
												intranet
											{/if}
											</span></a>
										{/if}
									{/foreach}
								{else}
								<a href="https://intranet.canada.ca/index-eng.asp"><span><span class="bold-gc">GC</span> intranet</span></a>
								{/if}
							</div>
						</section>
						{if $GCToolsModal == true}
