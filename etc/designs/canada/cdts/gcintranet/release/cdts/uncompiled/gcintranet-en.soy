							<div class="row">
								<div class="col-md-12">
									<ul class="list-inline margin-bottom-none gcbarlng">
										{foreach $link in $lngLinks}
											<li><a hreflang="{$link.lang}" lang="{$link.lang}" href="{$link.href}">{$link.text}</a></li>
										{/foreach}
									</ul>
								</div>
							</div>
						</section>
						{/if}
						{if $search != false}
							<section id="wb-srch" class="col-lg-3 col-md-4 hidden-sm hidden-xs">
								<h2>Search</h2>
								{if isNonnull($customSearch)}
								{foreach $item in $customSearch}
									{if isFirst($item)}
									<form action="
									{if isNonnull($item.action)}
										{$item.action}
									{else}
										https://intranet.canada.ca/search-recherche/query-recherche-eng.aspx
									{/if}
									" method="
									{if isNonnull($item.method)}
										{$item.method}
									{else}
										get
									{/if}
									" name="
									{if isNonnull($item.name)}
										{$item.name}
									{else}
										cse-search-box
									{/if}
									" role="search" class="form-inline">
										<div class="form-group">
											<label for="
											{if isNonnull($item.id)}
												{$item.id}
											{else}
												wb-srch-q
											{/if}
											" class="wb-inv">
											{if isNonnull($item.placeholder)}
												{$item.placeholder}
											{else}
												Search GCintranet
											{/if}
											</label>
											<input class="wb-srch-q form-control pull-right" id="
											{if isNonnull($item.id)}
												{$item.id}
											{else}
												wb-srch-q
											{/if}
											" name="
											{if isNonnull($item.name)}
												{$item.name}
											{else}
												q
											{/if}
											"  value="" size="21" maxlength="150" placeholder="
											{if isNonnull($item.placeholder)}
												{$item.placeholder}
											{else}
												Search GCintranet
											{/if}
											">
											{if isNonnull($item.hiddenInput)}
											{foreach $input in $item.hiddenInput}
												<input type="hidden" name="{$input.name}" value="{$input.value}" />
											{/foreach}
											{else}
												<input type="hidden" name="a" value="s" />
												<input type="hidden" name="s" value="1" />
												<input type="hidden" name="chk2" value="True" />
											{/if}
										</div>
									{/if}
								{/foreach}
								{else}
									<form action="https://intranet.canada.ca/search-recherche/query-recherche-eng.aspx" method="get" name="cse-search-box" role="search" class="form-inline">
										<div class="form-group">
											<label for="wb-srch-q" class="wb-inv">Search GCintranet</label>
											<input class="wb-srch-q form-control pull-right" id="wb-srch-q" name="q"  value="" size="21" maxlength="150" placeholder="Search GCintranet">
											<input type="hidden" name="a" value="s" />
											<input type="hidden" name="s" value="1" />
											<input type="hidden" name="chk2" value="True" />
										</div>
								{/if}
										<div class="form-group submit">&nbsp;
											<button type="submit" id="wb-srch-sub" class="btn btn-primary btn-small" name="wb-srch-sub"><span class="glyphicon-search glyphicon"></span><span class="wb-inv">Search</span></button>
										</div>
									</form>
							</section>
						{/if}
						<section id="wb-glb-mn" class="wb-mb-links col-xs-4 col-sm-3 visible-sm visible-xs">
							<h2>
							{if $search == false}
								Menu{if isNonnull($topSecMenu)}s{/if}
							{else}
								Search and menu{if isNonnull($topSecMenu)}s{/if}
							{/if}
							</h2>
							<ul class="list-inline text-right chvrn">
								<li><a href="#mb-pnl" title="
								{if $search == false}
									Menu{if isNonnull($topSecMenu)}s{/if}
								{else}
									Search and menu{if isNonnull($topSecMenu)}s{/if}
								{/if}
								" aria-controls="mb-pnl" class="overlay-lnk" role="button">
									{if $search != false}<span class="glyphicon glyphicon-search">{/if}
									<span class="glyphicon glyphicon-th-list">
									<span class="wb-inv">
									{if $search == false}
										Menu{if isNonnull($topSecMenu)}s{/if}
									{else}
										Search and menu{if isNonnull($topSecMenu)}s{/if}
									{/if}
									</span>
									{if ($search == false)}</span>{/if}
									</span>
								</a></li>
							</ul>
							<div id="mb-pnl"></div>
						</section>
					</div>
					{if $GCToolsModal != true}
					<div id="tools-sm" class="row{if isNonnull($intranetTitle)}{else} visible-md{/if}">
						<section id="tool-container-sm" class="col-md-12 col-sm-12">
							<div class="pull-left tool-link"><a href="https://gcconnex.gc.ca/splash/" class="GCTBannerOnclick-eng"><span class="bold-gc">GC</span>connex</a></div>
							<div class="pull-left tool-link"><a href="http://www.gcpedia.gc.ca/wiki/?setlang=en" class="GCTBannerOnclick-eng"><span class="bold-gc">GC</span>pedia</a></div>
							<div class="pull-left tool-link"><a href="http://gcdirectory-gcannuaire.gc.ca/en/GCD/?pgid=002" class="GCTBannerOnclick-eng"><span class="bold-gc">GC</span>directory</a></div>
							<div class="pull-left tool-link"><a href="https://gccollab.ca/splash/" class="GCTBannerOnclick-eng"><span class="bold-gc">GC</span>collab</a></div>
						</section>
					</div>
					{/if}
				</div>
			</div>
		</div>
		{if $siteMenu != false}
		{if ($subTheme == 'esdc') or ($subTheme == 'labour')}
		<nav role="navigation" id="wb-sm" data-wb-ajax='{lb}"url": "{if isNonnull($menuPath)}{$menuPath}{else}http://esdc.prv/includes/edsc-esdc_prv/wet4/en/mega_menu/esdcmenu-eng.html{/if}", "type": "replace", "nocache": true {rb}' data-trgt="mb-pnl" class="wb-menu visible-md visible-lg" typeof="SiteNavigationElement">
			<div class="container nvbar">
				<h2>Topics menu</h2>
				<div class="row">
					<ul class="list-inline menu">
						<li><a href="http://hrsdc.prv/eng/department/index.page?">Our Department</a></li>
						<li><a href="http://hrsdc.prv/eng/branches/index.page?">Branches and Regions</a></li>
